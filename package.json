{
  "name": "mapscii",
  "version": "0.3.1",
  "description": "MapSCII is a Braille & ASCII world map renderer for your console, based on OpenStreetMap",
  "main": "main.js",
  "scripts": {
    "start": "parcel index.html",
    "build": "parcel build --public-url . index.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rastapasta/mapscii.git"
  },
  "bin": {
    "mapscii": "./bin/mapscii.sh"
  },
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "map",
    "console",
    "terminal",
    "ascii",
    "osm",
    "vectortile",
    "mbtiles"
  ],
  "author": "Michael Stra√üburger <codepoet@cpan.org>",
  "license": "MIT",
  "dependencies": {
    "@mapbox/vector-tile": "^1.3.1",
    "buffer": "^6.0.3",
    "bresenham": "0.0.4",
    "earcut": "^2.2.2",
    "env-paths": "^2.2.0",
    "isomorphic-fetch": "^3.0.0",
    "keypress": "^0.2.1",
    "node-fetch": "^2.6.1",
    "parcel": "^1.12.4",
    "pbf": "^3.2.1",
    "rbush": "^3.0.1",
    "regenerator-runtime": "^0.13.7",
    "simplify-js": "^1.2.4",
    "string-width": "^4.2.0",
    "term-mouse": "^0.2.2",
    "x256": "0.0.2",
    "xterm": "^4.9.0",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "eslint": "^7.8.1",
    "eslint-plugin-jest": "^24.0.0",
    "jest": "^26.4.2",
    "parcel-bundler": "^1.12.4"
  }
}
